<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jy.user.dao.UserDao" >

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        u.id, u.user_name , u.tel , u.reg_time ,
        u.create_user_id , u.update_user_id,u.update_time , u.create_time
    </sql>


    <!--   分页查询用户信息-->
    <select id="queryPageList" resultType="com.jy.common.pojo.User">

        SELECT <include refid="Base_Column_List"/>
        FROM  sys_user u
        <where>
            <if test="keyword != null and keyword != '' ">
                and u.tel like concat(#{keyWord},"%")
            </if>
        </where>



    </select>

</mapper>